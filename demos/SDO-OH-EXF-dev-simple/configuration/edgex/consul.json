{
	"org": "${HZN_ORG_ID}",
	"url": "intel.edgex-consul",
	"version": "1.10.3",
	"arch": "amd64",
	"public": false,
	"sharable": "singleton",
	"requiredServices": [],
	"userInput": [],
	"deployment": {
		"services": {
			"edgex-core-consul": {
				"image": "consul:1.10.3",
				"command" : "agent -ui -bootstrap -server -client 0.0.0.0",
				"binds": [
					"consul-config:/consul/config:rw",
					"consul-data:/consul/data:rw"
				],
				"ports": [{
						"HostIP": "127.0.0.1",
						"HostPort": "8500:8500/tcp"
					}
				]
			}
		}
	}
}