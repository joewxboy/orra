{
	"org": "${HZN_ORG_ID}",
	"url": "intel.edgex-redis",
	"version": "6.2.6",
	"arch": "amd64",
	"public": false,
	"sharable": "singleton",
	"requiredServices": [],
	"userInput": [],
	"deployment": {
		"services": {
			"edgex-redis": {
				"image": "redis:6.2.6-alpine",
				"binds": [
					"db-data:/data:rw"
				],
				"ports": [{
					"HostIP": "127.0.0.1",
					"HostPort": "6379:6379/tcp"
				}]
			}
		}
	}
}